{
    "type": "group",
    "id": "_root",
    "bindings": {
        "width": "100%",
        "height": "100%"
    },
    "childrens": [
        {
            "type": "group",
            "id": "_dropDownGroup",
            "bindings": {
                "x": "center"
            }
        },
        {
            "type": "group",
            "id": "_listGroup",
            "bindings": {
                "width": "100%",
                "height": "100% - prevHeight - 10hdp",
                "y": "prevEndY + 8hdp"
            }
        }
    ]
}