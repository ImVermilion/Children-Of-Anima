{
    "type": "screen",
    "childrens": [{
        "type": "rect",
        "parameters": "fillColor:#000000;fillAlpha:0.65;strokeWidth:0",
        "bindings": {
            "size": {
                "width": "100%",
                "height": "100%"
            }
        },
        "childrens": [{
            "type": "rect",
            "parameters": "fillColor:#5c2313;fillAlpha:0.8;strokeWidth:2;corners:10",
            "effects": ["shadow", "glow"],
            "bindings": {
                "position": {
                    "x": "center",
                    "y": "center"
                },
                "size": {
                    "width": "60%",
                    "height": "35%"
                }
            },
            "childrens": [{
                    "type": "text",
                    "id": "questionTextSpr",
                    "parameters": "textColor:#bcac9a",
                    "bindings": {
                        "position": {
                            "x": "center",
                            "y": "20hdp"
                        },
                        "size": {
                            "width": "100%",
                            "height": "20%"
                        },
                        "text": ["%1", "$questionText"],
                        "fontSize": "12%"
                    }
                },
                {
                    "type": "group",
                    "bindings": {
                        "position": {
                            "x": "center",
                            "y": "bottom - 10hdp"
                        },
                        "size": {
                            "width": "80%",
                            "height": "30%"
                        }
                    },
                    "childrens": [{
                            "type": "button",
                            "id": "yesButton",
                            "parameters": {
                                "folderName": "pSimpleSkillsTree",
                                "imageName": "buttonNew",
                                "imageMargins": 28,
                                "clickSe": "Cursor1",
                                "keyboardKey": "ok",
                                "tintAlpha": 0.15
                            },
                            "bindings": {
                                "position": {
                                    "x": 0,
                                    "y": 0
                                },
                                "size": {
                                    "width": "40%",
                                    "height": "100%"
                                }
                            },
                            "childrens": [{
                                "type": "text",
                                "parameters": "textColor:#eac98a",
                                "bindings": {
                                    "position": {
                                        "x": "center",
                                        "y": "center"
                                    },
                                    "size": {
                                        "width": "100%",
                                        "height": "100%"
                                    },
                                    "text": "YES",
                                    "fontSize": "45%"
                                }
                            }]
                        },
                        {
                            "type": "button",
                            "id": "noButton",
                            "parameters": {
                                "folderName": "pSimpleSkillsTree",
                                "imageName": "buttonNew",
                                "imageMargins": 28,
                                "clickSe": "Cursor1",
                                "keyboardKey": "",
                                "tint": "#d91111",
                                "overTint": "#f72a2a",
                                "activeTint": "#d91111",
                                "tintAlpha": 0.25
                            },
                            "bindings": {
                                "position": {
                                    "x": "right",
                                    "y": "prevY"
                                },
                                "size": {
                                    "width": "40%",
                                    "height": "100%"
                                }
                            },
                            "childrens": [{
                                "type": "text",
                                "parameters": "textColor:#eac98a",
                                "bindings": {
                                    "position": {
                                        "x": "center",
                                        "y": "center"
                                    },
                                    "size": {
                                        "width": "100%",
                                        "height": "100%"
                                    },
                                    "text": "NO",
                                    "fontSize": "45%"
                                }
                            }]
                        }
                    ]
                }
            ]
        }]
    }]
}